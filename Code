import random
caracteres = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

pregunt = int(input("Cuantos caracteres quieres que tenga tu contrase√±a?"))
password = ""

for i in range(pregunt):
    y= random.randint(0,len(caracteres))
    password += caracteres[y]

print(password)
